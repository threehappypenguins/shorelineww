{
  "entryPoints": ["src"],
  "entryPointStrategy": "expand",
  "out": "docs",
  "theme": "default",
  "includeVersion": true,
  "excludePrivate": true,
  "excludeProtected": false,
  "excludeInternal": true,
  "excludeExternals": true,
  "readme": "none",
  "sort": ["source-order"],
  "validation": {
    "invalidLink": true
  },
  "searchInComments": true,
  "exclude": ["**/*.md", "**/node_modules/**", "**/dist/**", "**/build/**", "**/doc-site/**", "**/tests/**", "**/__tests__/**", "**/*.test.tsx", "**/*.spec.tsx"]
}


