{
  "name": "final-project-group3",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "postinstall": "prisma generate",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "docs": "pnpm run docs:api && pnpm run docs:frontend",
    "docs:api": "typedoc --options typedoc.json -out doc-site/public/api --exclude \"**/*.test.tsx\" --exclude \"**/*.test.ts\" --entryPoints src/app/api",
    "docs:frontend": "typedoc --options typedoc.json --exclude \"**/src/app/api/**\" --exclude \"**/*.test.tsx\" --exclude \"**/*.test.ts\" --out \"doc-site/public/frontend\"",
    "docs:dev": "pnpm run copy:guides && pnpm run docs && vitepress dev doc-site",
    "docs:build": "pnpm run copy:guides && pnpm run docs && vitepress build doc-site",
    "docs:preview": "vitepress preview doc-site",
    "copy:guides": "node -e \"const fs=require('fs');const path=require('path');const src='docs';const dest='doc-site/guides';fs.rmSync(dest,{recursive:true,force:true});fs.mkdirSync(dest,{recursive:true});fs.readdirSync(src).forEach(f=>fs.cpSync(path.join(src,f),path.join(dest,f),{recursive:true}));\""
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.11.1",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@prisma/client": "^5.22.0",
    "cloudinary": "^2.9.0",
    "next": "16.1.6",
    "next-auth": "5.0.0-beta.30",
    "next-cloudinary": "^6.17.5",
    "next-themes": "^0.4.6",
    "react": "19.2.3",
    "react-dom": "19.2.3"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^5.1.3",
    "cypress": "^15.10.0",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "jsdom": "^28.0.0",
    "prisma": "^5.22.0",
    "tailwindcss": "^4",
    "tsx": "^4.21.0",
    "typedoc": "^0.28.17",
    "typescript": "^5",
    "vitepress": "2.0.0-alpha.16",
    "vitest": "^4.0.18"
  },
  "pnpm": {
    "overrides": {
      "@auth/core": "0.41.1"
    },
    "onlyBuiltDependencies": [
      "@prisma/client",
      "@prisma/engines",
      "cypress",
      "esbuild",
      "prisma"
    ]
  }
}